	Keywords = {exon-junction complex,nonsense mutation,p-body,rna decay,rna splicing,translation},
	Mendeley-Groups = {Genetic Diseases},
	Number = {1},
	Pages = {51--74},
	Pmid = {17352659},
	Title = {{The Nonsense-Mediated Decay RNA Surveillance Pathway}},
