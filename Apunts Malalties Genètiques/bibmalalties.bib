	Isbn = {0148-7299 (Print)$\backslash$r0148-7299 (Linking)},
	Issn = {01487299},
	Journal = {American Journal of Medical Genetics - Seminars in Medical Genetics},
	Keywords = {Angelman syndrome,Behavioral phenotype,Chromosome disorder,Genetic imprinting,Mental retardation,Microdeletion,Prader-Willi syndrome,Uniparental disomy},
	Mendeley-Groups = {Genetic Diseases},
	Number = {2},
