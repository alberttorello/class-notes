	Isbn = {1934-5909},
	Issn = {19345909},
	Journal = {Cell Stem Cell},
	Keywords = {DNA,STEMCELL},
	Mendeley-Groups = {Cell and Gene Therapy},
	Number = {1},
	Pages = {55--70},
