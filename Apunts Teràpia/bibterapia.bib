	Journal = {Expert Opinion on Biological Therapy},
	Keywords = {12,155-163,2,2012,adipogenesis,adipose derived stem cells,autologous fat grafting,biol,cell-assisted,expert opin,lipotransfer,neuropeptide y,stem cells,ther},
	Mendeley-Groups = {Cell and Gene Therapy},
	Number = {2},
